/* Program to chech whether numbers are equal or not */
#include<stdio.h>
int main()
{
    int n1 = 0;
    int n2 = 0;
    printf("Enter first number :\n");
    scanf("%d",&n1);
    printf("Enter second number :\n");
    scanf("%d",&n2);
    if(n1 == n2)
    {
        printf("Numbers are same");
    }
    else
    {
        printf("Numbers are not same");
    }
    return 0;
}
