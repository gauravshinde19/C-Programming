/* Program to print even factors */
#include<stdio.h>
int main()
{
    int n = 0;
    int i = 0;
    int c = 0;
    printf("Enter the number :\n");
    scanf("%d",&n);
    printf("Even Factors :\n");
    for(i=1; i<=n; i++)
    {
        if(n % i == 0)
        {
            if(i % 2 == 0)
            {
                printf("%d\n",i);
            }
        }
    }
    return 0;
}
