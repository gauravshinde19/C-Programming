/* Program to check number is prime or not */
#include<stdio.h>
int main()
{
    int n = 0;
    int i = 0;
    int c = 0;
    printf("enter the number :\n");
    scanf("%d",&n);
    for(i=1; i<=n; i++)
    {
        if(n % i == 0)
        {
            c++;
        }
    }
    if(c == 2)
    {
        printf("prime");
    }
    else
    {
        printf("not prime");
    }
}
