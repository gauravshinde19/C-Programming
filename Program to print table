/* Write a program which accept number from user and display its table */
#include<stdio.h>
int main()
{
    int n = 0;
    int i = 0;
    int mul = 1;
    printf("Enter the number :\n");
    scanf("%d",&n);   
    printf("Table :\n");
        for(i=1; i<=10; i++)
        {
            mul = n * i;
            printf("%d\n",mul);            
        }              
    return 0;
}
