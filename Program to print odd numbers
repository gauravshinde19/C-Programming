/* Program to print odd numbers */
#include<stdio.h>
int main()
{
    int n = 0;
    int i = 0;
    printf("enter the number :\n");
    scanf("%d",&n);
    printf("odd numbers below %d :\n",n);
    for(i=1; i<=n; i++)
    {
        if(i % 2 == 1)
        {
            printf("%d\n",i);
        }
    }
}
